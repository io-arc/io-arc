    "eslint": "7.6.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-prettier": "3.1.4",
    "graphql": "15.3.0",
    "graphql-tag": "2.11.0",
    "jest": "25.1.0",
    "jquery": "3.5.1",
    "lerna": "^3.20.2",
    "lodash": "4.17.21",
    "prettier": "^2.0.5",
    "rollup": "2.23.1",
    "rollup-plugin-commonjs": "10.1.0",
